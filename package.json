    "cheerio": "^0.22.0",
    "cross-env": "^3.1.3",
    "css-loader": "^2.1.1",
    "fstream": "^1.0.10",
    "jest": "^24.5.0",
    "node-zip": "^1.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.3.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "svgfont2js": "^0.1.2",
