  "description": "React Icons Kit using various Svg Icon set",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "react",
    "react icons",
    "react svg icons",
    "fontawesome",
    "material design icons",
    "icons",
    "icons react",
    "react-icons",
    "react-icons-kit"
  ],
  "author": "Warren Mira <warrenmira@gmail.com>",
